{
  "extends": "@saasfly/typescript-config/base.json",
  "compilerOptions": {
    "tsBuildInfoFile": ".next/tsconfig.tsbuildinfo",
    "types": ["next", "next/types/global"],
    "plugins": [
      {
        "name": "next"
      }
    ],
    "paths": {
      "~/*": ["./src/*"],
      "@/*": ["./src/*"]
    }
  },
  "include": ["next-env.d.ts", "**/*.ts", "**/*.tsx", ".next/types/**/*.ts"],
  "exclude": [
    "node_modules",
    "src/components/uniswap_buy(tofix).tsx",
    "src/components/uniswapclon.tsx"
  ]
}
